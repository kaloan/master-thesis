      \strng{bibnamehash}{9201bc51958fae40fd0e3f6e39ff9d49}
      \strng{authorbibnamehash}{9201bc51958fae40fd0e3f6e39ff9d49}
      \strng{authornamehash}{9201bc51958fae40fd0e3f6e39ff9d49}
