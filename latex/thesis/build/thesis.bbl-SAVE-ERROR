      \field{pages}{29\bibrangedash 48}
      \range{pages}{20}
      \verb{doi}
      \verb 10.1016/S0025-5564(02)00108-6
      \endverb
      \keyw{Basic reproduction number,Sub-threshold equilibrium,Disease transmission model,Disease control}
